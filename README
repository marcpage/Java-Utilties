
# bash -e README

mkdir -p documentation/html/implementation
javadoc -d documentation/html *.java -quiet -link http://download.oracle.com/javase/1,5.0/docs/api; javadoc -d documentation/html/implementation *.java -private -quiet -link http://download.oracle.com/javase/1,5.0/docs/api

javac -source 1.5 *.java 2>&1 | edit
